{
  "name": "react-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "set PORT=9000 && node scripts/start.js",
    "serve": "set PORT=9000 && node scripts/start.js",
    "dev": "set PORT=9000 && node scripts/start.js",
    "lint": "eslint --fix --ext .js src",
    "dll": "webpack --config webpack.config.dll.js",
    "build:windows": "set GENERATE_SOURCEMAP=false&& node --max-old-space-size=4096 scripts/build.js",
    "build:mac": "env GENERATE_SOURCEMAP=false npm run build",
    "test": "node scripts/test.js",
    "color": "node color.js",
    "new": "plop"
  },
  "dependencies": {
    "@redux-saga/simple-saga-monitor": "^1.0.2",
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.25",
